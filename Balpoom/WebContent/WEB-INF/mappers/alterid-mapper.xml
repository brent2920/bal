<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="alter">

<select id="agent" parameterType="java.util.HashMap" resultType="java.util.HashMap">
	select * from agent where id = #{alterid} and sell_num =#{sell_num}
</select>

<select id="person" parameterType="java.lang.String" resultType="java.util.HashMap">
	select * from join where EEMAIL = #{alterid}
</select>

</mapper>